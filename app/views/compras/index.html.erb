<p id="notice"><%= notice %></p>

<h1>Compras</h1>

<table>
  <thead>
    <tr>
      <th>Hitorico</th>
      <th>Codigonf</th>
      <th>Datacompra</th>
      <th>Valor</th>
      <th>Parcelamento</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @compras.each do |compra| %>
      <tr>
        <td><%= compra.hitorico %></td>
        <td><%= compra.codigoNF %></td>
        <td><%= compra.dataCompra %></td>
        <td><%= compra.valor %></td>
        <td><%= compra.parcelamento %></td>
        <td><%= link_to 'Show', compra %></td>
        <td><%= link_to 'Edit', edit_compra_path(compra) %></td>
        <td><%= link_to 'Destroy', compra, method: :delete, data: { confirm: 'Are you sure?' } %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Compra', new_compra_path %>
